#include <stdio.h>
#include <stdlib.h>
int main()
{
	system("chcp 1251");
	system("cls");
	int N, i;
	printf("Введите размер массива\n");
	scanf_s("%d", &N);
	int* A = new int[N];
	printf("Введите элементы массива A\n");
	for (i = 0; i < N; i++) {
		scanf_s("%d", &A[i]);
	}
	int K = 0;
	for (i = 1; i < N; i++) {
		if (A[K] != A[i]) {
			K++;
			A[K] = A[i];
		}
	}
	K++;
	for (i = 0; i < K; i++) {
		printf("%d ", A[i]);
	}
	return 0;
}
