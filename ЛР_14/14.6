#include <stdio.h>
#include <stdlib.h>
int main()
{
	system("chcp 1251");
	system("cls");
	int N;
	printf("Введите число Фибоначчи\n");
	scanf_s("%d", &N);
	int i = 2;
	int a = 0, b = 1, c = 1;
	while (N > b) {
		c = a + b;
		a = b;
		b = c;
		i++;
	}
	printf("Порядковый номер: %d", i);
	return 0;
}
