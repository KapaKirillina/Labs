#include <stdio.h>
#include <stdlib.h>
int main()
{
	system("chcp 1251");
	system("cls");
	int N, i, j;
	printf("Введите размер массива\n");
	scanf_s("%d", &N);
	int* A = new int[N];
	printf("Введите элементы массива\n");
	for (i = 1; i <= N; i++) {
		scanf_s("%d", &A[i]);
	}
	for (i = 1; i < N; i++) {
		for (j = i + 1; j <= N;  j++) {
			if (A[i] == A[j]) {
				if (i < j) {
					printf("%d %d", i, j);
				}
				else {
					printf("%d %d", j, i);
				}
				break;
			}
		}
	}
	return 0;
}
