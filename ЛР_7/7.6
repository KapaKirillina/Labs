#include <stdio.h>
#include <stdlib.h>
int main()
{
	system("chcp 1251");
	system("cls");
	float a1, a2, b1, b2, c1, c2;
	float x, y;
	printf("Введите коэффициенты a1, b1, c1\n");
	scanf_s("%f %f %f", &a1, &b1, &c1);
	printf("Введите коэффициенты a2, b2, c2\n");
	scanf_s("%f %f %f", &a2, &b2, &c2);
	float d = a1 * b2 - a2 * b1;
	float dx = c1 * b2 - c2 * b1;
	float dy = a1 * c2 - a2 * c1;
	if (d == 0) {
			printf("Решений нет");
	}
	else {
		x = dx / d;
		y = dy / d;
		printf("x = %f, y = %f", x, y);
	}
	return 0;
}
