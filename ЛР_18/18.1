#include <stdio.h>
#include <stdlib.h>
int main()
{
	system("chcp 1251");
	system("cls");
	int N, i;
	printf("Введите размер массивов\n");
	scanf_s("%d", &N);
	int* A = new int[N];
	int* B = new int[N];
	printf("Введите элементы массива A\n");
	for (i = 0; i < N; i++) {
		scanf_s("%d", &A[i]);
	}
	printf("Введите элементы массива B\n");
	for (i = 0; i < N; i++) {
		scanf_s("%d", &B[i]);
	}
	int temp;
	for (i = 0; i < N; i++) {
		temp = A[i];
		A[i] = B[i];
		B[i] = temp;
	}
	printf("Элементы преобразованного массива A: ");
	for (i = 0; i < N; i++) {
		printf("%d ", A[i]);
	}
	printf("\nЭлементы преобразованного массива B: ");
	for (i = 0; i < N; i++) {
		printf("%d ", B[i]);
	}
	return 0;
}
