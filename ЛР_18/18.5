#include <stdio.h>
#include <stdlib.h>
int main()
{
	system("chcp 1251");
	system("cls");
	int N, i;
	printf("Введите размер массива\n");
	scanf_s("%d", &N);
	int* A = new int[N];
	printf("Введите элементы массива A\n");
	for (i = 0; i < N; i++) {
		scanf_s("%d", &A[i]);
	}
	int temp, temp1;
	for (i = 0; i < N; i++) {
		if (A[0] < A[i]) {
			temp = A[i];
			A[i] = A[0];
			int j = i + 1;
			for (i = j; i <= N; i++) {
				temp1 = A[i];
				A[i] = temp;
				temp = temp1;
			}
		}

	}
	for (i = 1; i <= N; i++) {
		printf("%d ", A[i]);
	}
	return 0;
}
